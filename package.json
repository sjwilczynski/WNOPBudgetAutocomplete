{
  "name": "wnop-budget-task-pane-add-in",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/sjwilczynski/WNOPBudgetAutocomplete"
  },
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "config": {
    "app_to_debug": "excel",
    "app_type_to_debug": "desktop",
    "dev_server_port": 3000
  },
  "scripts": {
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "dev-server": "vite",
    "lint": "office-addin-lint check",
    "lint:fix": "office-addin-lint fix",
    "prettier": "office-addin-lint prettier",
    "size": "size-limit",
    "start": "office-addin-debugging start manifest.xml",
    "start:desktop": "office-addin-debugging start manifest.xml desktop",
    "start:web": "office-addin-debugging start manifest.xml web",
    "stop": "office-addin-debugging stop manifest.xml",
    "validate": "office-addin-manifest validate manifest.xml",
    "watch": "vite",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest run"
  },
  "dependencies": {
    "@fluentui/react-components": "^9.60.0",
    "@fluentui/react-datepicker-compat": "^0.5.5",
    "@fluentui/react-icons": "^2.0.290",
    "@hookform/resolvers": "^2.9.5",
    "@tanstack/react-query": "^5.70.0",
    "i18next": "^22.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^5.0.0",
    "react-hook-form": "^7.40.0",
    "react-i18next": "^11.15.3",
    "yup": "^0.32.11",
    "yup-locale-pl": "^0.1.3"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.5",
    "@size-limit/preset-app": "^8.1.0",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/experimental-addon-test": "^8.6.12",
    "@storybook/react": "^8.6.12",
    "@storybook/react-vite": "^8.6.12",
    "@storybook/test": "^8.6.12",
    "@types/office-js": "^1.0.256",
    "@types/office-runtime": "^1.0.23",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.11",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/browser": "^3.1.2",
    "@vitest/coverage-v8": "^3.1.2",
    "chromatic": "^11.27.0",
    "eslint": "^9.21.0",
    "eslint-plugin-office-addins": "^4.0.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-storybook": "^0.11.3",
    "globals": "^16.0.0",
    "mockdate": "^3.0.5",
    "msw": "^2.7.5",
    "msw-storybook-addon": "^2.0.4",
    "office-addin-cli": "^1.5.3",
    "office-addin-debugging": "^4.6.3",
    "office-addin-dev-certs": "^1.9.3",
    "office-addin-lint": "^3.0.0",
    "office-addin-manifest": "^1.10.4",
    "office-addin-prettier-config": "^2.0.0",
    "playwright": "^1.50.1",
    "process": "^0.11.10",
    "size-limit": "^8.1.0",
    "storybook": "^8.6.12",
    "typescript": "^4.9.1",
    "typescript-eslint": "^8.26.0",
    "vite": "^6.3.3",
    "vite-plugin-static-copy": "^2.3.1",
    "vitest": "^3.1.2"
  },
  "prettier": "office-addin-prettier-config",
  "browserslist": [
    "defaults"
  ],
  "size-limit": [
    {
      "path": "dist/assets/*.js",
      "limit": "260 kB"
    }
  ],
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
