{
  "name": "wnop-budget-task-pane-add-in",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/sjwilczynski/WNOPBudgetAutocomplete"
  },
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "config": {
    "app_to_debug": "excel",
    "app_type_to_debug": "desktop",
    "dev_server_port": 3000
  },
  "scripts": {
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "dev-server": "vite",
    "lint": "office-addin-lint check",
    "lint:fix": "office-addin-lint fix",
    "prettier": "office-addin-lint prettier",
    "size": "size-limit",
    "start": "office-addin-debugging start manifest.xml",
    "start:desktop": "office-addin-debugging start manifest.xml desktop",
    "start:web": "office-addin-debugging start manifest.xml web",
    "stop": "office-addin-debugging stop manifest.xml",
    "validate": "office-addin-manifest validate manifest.xml",
    "watch": "vite",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest run"
  },
  "dependencies": {
    "@fluentui/react-components": "^9.68.3",
    "@fluentui/react-datepicker-compat": "^0.6.12",
    "@fluentui/react-icons": "^2.0.302",
    "@hookform/resolvers": "^5.1.0",
    "@tanstack/react-query": "^5.80.6",
    "i18next": "^25.2.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.57.0",
    "react-i18next": "^15.5.2",
    "storybook-addon-test-codegen": "^2.0.1",
    "yup": "^1.6.1",
    "yup-locale-pl": "^0.1.3"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.0",
    "@size-limit/preset-app": "^11.2.0",
    "@storybook/addon-a11y": "9.1.2",
    "@storybook/addon-vitest": "9.1.2",
    "@storybook/react-vite": "^9.0.6",
    "@types/office-js": "^1.0.508",
    "@types/office-runtime": "^1.0.35",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "ajv": "^8.17.1",
    "chromatic": "^13.1.3",
    "eslint": "^9.33.0",
    "eslint-plugin-office-addins": "^4.0.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-storybook": "^9.1.2",
    "globals": "^16.3.0",
    "mockdate": "^3.0.5",
    "msw": "^2.10.5",
    "msw-storybook-addon": "^2.0.5",
    "office-addin-cli": "^2.0.3",
    "office-addin-debugging": "^6.0.3",
    "office-addin-dev-certs": "^2.0.3",
    "office-addin-lint": "^3.0.3",
    "office-addin-manifest": "^2.0.3",
    "office-addin-prettier-config": "^2.0.1",
    "playwright": "^1.54.2",
    "process": "^0.11.10",
    "size-limit": "^11.2.0",
    "storybook": "^9.1.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1",
    "vite": "^6.3.5",
    "vite-plugin-checker": "^0.10.2",
    "vite-plugin-static-copy": "^3.1.1",
    "vitest": "^3.2.4"
  },
  "prettier": "office-addin-prettier-config",
  "browserslist": [
    "defaults"
  ],
  "size-limit": [
    {
      "path": "dist/assets/*.js",
      "limit": "247 kB",
      "gzip": true
    }
  ],
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
