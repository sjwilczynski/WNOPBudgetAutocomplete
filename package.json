{
  "name": "wnop-budget-task-pane-add-in",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/sjwilczynski/WNOPBudgetAutocomplete"
  },
  "license": "MIT",
  "config": {
    "app_to_debug": "excel",
    "app_type_to_debug": "desktop",
    "dev_server_port": 3000
  },
  "scripts": {
    "build": "webpack --mode production && npm run size",
    "build:dev": "webpack --mode development",
    "dev-server": "webpack serve --mode development",
    "generate-bundle-stats": "webpack --profile --json > stats.json",
    "lint": "office-addin-lint check",
    "lint:fix": "office-addin-lint fix",
    "prettier": "office-addin-lint prettier",
    "size": "size-limit",
    "start": "office-addin-debugging start manifest.xml",
    "start:desktop": "office-addin-debugging start manifest.xml desktop",
    "start:web": "office-addin-debugging start manifest.xml web",
    "stop": "office-addin-debugging stop manifest.xml",
    "validate": "office-addin-manifest validate manifest.xml",
    "watch": "webpack --mode development --watch"
  },
  "dependencies": {
    "@fluentui/react-components": "^9.9.0",
    "@fluentui/react-icons": "^2.0.100",
    "@hookform/resolvers": "^2.8.8",
    "i18next": "^21.6.6",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-hook-form": "^7.20.0",
    "react-i18next": "^11.15.3",
    "yup": "^0.32.11",
    "yup-locale-pl": "^0.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.13.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.4",
    "@size-limit/preset-app": "^8.1.0",
    "@types/office-js": "^1.0.180",
    "@types/office-runtime": "^1.0.17",
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
    "@types/webpack": "^4.4.34",
    "@types/webpack-dev-server": "^4.1.0",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.5.1",
    "eslint": "^7.32.0",
    "eslint-plugin-office-addins": "^1.1.4",
    "eslint-plugin-react": "^7.16.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.2",
    "office-addin-cli": "^1.3.4",
    "office-addin-debugging": "^4.3.7",
    "office-addin-dev-certs": "^1.7.6",
    "office-addin-lint": "^1.4.5",
    "office-addin-manifest": "^1.7.6",
    "office-addin-prettier-config": "^1.1.3",
    "process": "^0.11.10",
    "react-refresh": "^0.11.0",
    "size-limit": "^8.1.0",
    "style-loader": "^3.3.1",
    "type-fest": "^2.10.0",
    "typescript": "^4.3.5",
    "webpack": "^5.50.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.1.1"
  },
  "prettier": "office-addin-prettier-config",
  "browserslist": [
    "defaults"
  ],
  "size-limit": [
    {
      "path": "dist/*.js",
      "limit": "183 kB"
    }
  ]
}
